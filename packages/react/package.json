{
  "name": "@modalor/react",
  "type": "module",
  "version": "1.0.0",
  "packageManager": "pnpm@10.5.0",
  "description": "A modal state manager for React.",
  "author": "Croatia Lu <wuli.croatia@foxmail.com>",
  "license": "MIT",
  "funding": "https://github.com/sponsors/croatialu",
  "homepage": "https://github.com/croatialu/modalor#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/croatialu/modalor.git"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org",
    "access": "public",
    "tag": "latest"
  },
  "bugs": "https://github.com/croatialu/modalor/issues",
  "keywords": [
    "vue",
    "modal",
    "modalor"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --watch",
    "lint": "eslint .",
    "prepublishOnly": "nr build",
    "release": "bumpp && npm publish",
    "start": "esno src/index.ts",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "prepare": "simple-git-hooks"
  },
  "peerDependencies": {
    "react": ">=16.8.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^4.3.0",
    "@antfu/ni": "^23.3.1",
    "@antfu/utils": "^9.1.0",
    "@types/node": "^22.13.5",
    "@types/react": "^19.0.10",
    "bumpp": "^10.0.3",
    "eslint": "^9.21.0",
    "esno": "^4.8.0",
    "lint-staged": "^15.4.3",
    "pnpm": "^10.5.0",
    "react": "^19.0.0",
    "simple-git-hooks": "^2.11.1",
    "typescript": "^5.7.3",
    "unbuild": "^3.3.1",
    "vite": "^6.2.0",
    "vitest": "^3.0.7"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
